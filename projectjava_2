public class Mavenproject1 {

   
    public static void main(String[] args) {
         int a,b,c;
          System.out.println("Enter first number:");
          Scanner s=new Scanner(System.in);
          a=s.nextInt();
          System.out.println("Enter Second number:");
          b=s.nextInt();
          
          c=a+b;
          
          System.out.println("SUM: " +c);
          
    }
}
